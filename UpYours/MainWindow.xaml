<Window x:Class="UpYours.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UpYours"
        xmlns:models="clr-namespace:UpYours.Models"
        xmlns:views="clr-namespace:UpYours.View"
        mc:Ignorable="d"  
        DataContext="{Binding Source={StaticResource Locator}, Path=Main}"
        x:Name="window"
        Height="457.269" Width="700">
    <Window.Resources>
        <ResourceDictionary>
            <DataTemplate  DataType="{x:Type models:Video}">
                <views:VideoView />
            </DataTemplate>
        </ResourceDictionary>
    </Window.Resources>
    <Grid>
        <ListBox x:Name="lbVideos" ItemsSource="{Binding Videos}" Margin="10,35,10,10" HorizontalContentAlignment="Stretch"/>
        <Button x:Name="btnAdd" Content="Button" HorizontalAlignment="Left" Margin="432,10,0,0" VerticalAlignment="Top" Width="75" Click="btnAdd_Click"/>

    </Grid>
</Window>
